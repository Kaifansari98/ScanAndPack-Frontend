{
  "expo": {
    "name": "Furnix",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/furnix.jpeg",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.vloq.furnixcrm"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/furnix.jpeg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.vloq.furnixcrm"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/furnix.jpeg"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/furnix.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "da5d46f1-4b71-4472-b393-cce3a3a00d6b"
      }
    }
  }
}
